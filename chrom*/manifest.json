{
    "name": "Quizizz - fetcher",
    "description": "Build an Extension!",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "./16.png",
            "32": "./32.png",
            "48": "./48.png",
            "128": "./128.png"
        }
    },
    "permissions": [
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://quizizz.com/*"
    ],
    "icons": {
      "16": "./16.png",
      "32": "./32.png",
      "48": "./48.png",
      "128": "./128.png"
    }
}